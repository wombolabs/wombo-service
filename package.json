{
  "name": "wombo-api-service",
  "version": "1.0.0",
  "description": "Wombo API Service",
  "author": "Wombo <dev@wombo.gg>",
  "private": true,
  "scripts": {
    "prisma:client": "npx prisma generate",
    "prisma:seed": "npx dotenv -e .env.local -- npx prisma db seed",
    "migrate:dev": "npx dotenv -e .env.local -- npx prisma migrate dev --name init",
    "migrate:deploy": "npx dotenv -e .env.test -- npx prisma migrate deploy",
    "subdomain:create": "npx dotenv -e .env.test -- npx serverless create_domain --stage test",
    "build:local": "serverless webpack --stage local",
    "build:test": "serverless webpack --stage test",
    "build:production": "serverless webpack --stage production",
    "deploy:offline": "node --inspect=localhost:9240 node_modules/.bin/serverless offline --stage local",
    "deploy:test": "serverless deploy --stage test",
    "deploy:staging": "serverless deploy --stage staging",
    "deploy:production": "serverless deploy --stage production",
    "release": "semantic-release",
    "lint": "npx eslint . --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "prettier:fix": "npx prettier --write src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wombolabs/wombo-service.git"
  },
  "bugs": {
    "url": "https://github.com/wombolabs/wombo-service/issues"
  },
  "homepage": "https://github.com/wombolabs/wombo-service#readme",
  "dependencies": {
    "@prisma/client": "4.0.0",
    "@sentry/integrations": "^7.2.0",
    "@sentry/serverless": "^7.2.0",
    "@sentry/tracing": "^7.2.0",
    "@vendia/serverless-express": "^4.8.2",
    "axios": "^0.27.2",
    "crypto": "^1.0.1",
    "express": "^4.18.1",
    "express-promise-router": "^4.1.1",
    "express-query-parser": "^1.3.3",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "moment-timezone": "^0.5.34",
    "openapi-validator-middleware": "^3.2.6",
    "ramda": "^0.28.0",
    "saslprep": "^1.0.3",
    "stripe": "^9.9.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@eclass/semantic-release-sentry-releases": "^3.0.0",
    "@google/semantic-release-replace-plugin": "^1.1.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prisma": "4.0.0",
    "semantic-release": "^19.0.3",
    "serverless": "^3.19.0",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-domain-manager": "^6.0.3",
    "serverless-offline": "^8.8.0",
    "serverless-webpack": "^5.7.1",
    "serverless-webpack-prisma": "^1.1.0",
    "webpack": "^5.73.0",
    "webpack-node-externals": "^3.0.0"
  },
  "prisma": {
    "seed": "node prisma/seed/index.js"
  },
  "lint-staged": {
    "*.js": [
      "npx prettier --write",
      "npx eslint --cache --fix"
    ]
  },
  "engines": {
    "node": "16.15.0",
    "npm": "8.5.5"
  },
  "packageManager": "npm@8.5.5"
}
